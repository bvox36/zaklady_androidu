<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tableLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:collapseColumns=""
    android:shrinkColumns="*"
    android:stretchColumns="*" >

    <TableRow
        android:id="@+id/tableRowNadpis"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal" >

        <TextView
            android:id="@+id/textViewNadpis"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_span="6"
            android:gravity="center"
            android:padding="10dp"
            android:text="Apple HTTP/HTTPS live streaming"
            android:textSize="18dp"
            android:textStyle="bold"
            android:typeface="serif" >
        </TextView>
    </TableRow>

    <TableRow
        android:id="@+id/tableRowAPI"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/TextView00"	
            android:text="" >
        </TextView>

        <TextView
            android:id="@+id/TextView01"
            android:gravity="center"
            android:text="API 2.2"
            android:textStyle="bold"
            android:typeface="serif" >
        </TextView>				

        <TextView
            android:id="@+id/TextView02"
            android:gravity="center"
            android:text="API 2.3"
            android:textStyle="bold"
            android:typeface="serif" >
        </TextView>

        <TextView
            android:id="@+id/TextView03"
            android:gravity="center"
            android:text="API 3.0"
            android:textStyle="bold"
            android:typeface="serif" >
        </TextView>

        <TextView
            android:id="@+id/TextView04"
            android:gravity="center"
            android:text="API 3.1"
            android:textStyle="bold"
            android:typeface="serif" >
        </TextView>

        <TextView
            android:id="@+id/textView05"
            android:gravity="center"
            android:text="API 4.0 +"
            android:textStyle="bold"
            android:typeface="serif" >
        </TextView>
    </TableRow>

    <TableRow
        android:id="@+id/tableRowHttp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/textView10"
            android:text="HTTP"
            android:textStyle="bold" >
        </TextView>

        <TextView
            android:id="@+id/textView11"
